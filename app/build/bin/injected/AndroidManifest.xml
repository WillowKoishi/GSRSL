<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="willow.getsimplerocketsship.lite"
	android:versionCode="6000"
	android:versionName="LitE 1.0">

	<uses-sdk
		android:minSdkVersion="14"
		android:targetSdkVersion="21"/>

	<application
		android:allowBackup="true"
		android:icon="@drawable/icon"
		android:label="@string/app_name"
		android:theme="@style/AppTheme">

		<activity
			android:windowSoftInputMode="adjustUnspecified|stateHidden"
			android:screenOrientation="portrait"
			android:name=".appcompat.AppCompatMain"
			android:label="@string/app_name">

			<intent-filter>

				<action
					android:name="android.intent.action.MAIN"/>

				<category
					android:name="android.intent.category.LAUNCHER"/>

			</intent-filter>

		</activity>

		<activity
			android:windowSoftInputMode="adjustUnspecified|stateHidden"
			android:screenOrientation="portrait"
			android:name=".appcompat.AppCompatSetting"
			android:theme="@style/AppTheme2"
			android:label="@string/app_name"/>

		<activity
			android:windowSoftInputMode="adjustUnspecified|stateHidden"
			android:screenOrientation="portrait"
			android:name=".appcompat.ThemeSettings"
			android:label="@string/app_name"/>

	</application>

</manifest>

